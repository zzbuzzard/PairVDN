{
	"env": "LunarLander-v2",
    "seed": 0,

    "batch_size": 32,
    "num_epochs": 100,
    "learning_rate": 1e-4,
    "display_every": 20,
	"save_every": 20,
	"eval_every": 5,
	"eval_reps": 16,

    "exp_buffer_len": 100000,
    "gamma": 0.99,
	"target_network_gamma": 0.99,
    "exploration_eps_start": 1,
    "exploration_eps_end": 0.05,
    "simulation_steps_per_epoch": 1000,
    "num_envs": 16,

	"model_type": "QMLP",
	"model_config": {
		"hidden_layers": [128, 128, 128]
	}
}